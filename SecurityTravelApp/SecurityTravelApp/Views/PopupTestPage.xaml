<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SecurityTravelApp.Views.PopupTestPage">
    <ContentPage.Content>
        <StackLayout Spacing="10" Margin="10" VerticalOptions="Center">
            <Button Text="record and call" Clicked="Button_Clicked" HorizontalOptions="Start" HeightRequest="50"/>
            <Button Text="add audio and toggle sendable" Clicked="Button2_Clicked" HorizontalOptions="Start" HeightRequest="50"/>
            <Button Text="launch sync" Clicked="Button3_Clicked" HorizontalOptions="Start" HeightRequest="50"/>
            <Button Text="track user" Clicked="Button4_Clicked" HorizontalOptions="Start" HeightRequest="50"/>
            <Button Text="stop tracking user" Clicked="Button5_Clicked" HorizontalOptions="Start" HeightRequest="50"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>